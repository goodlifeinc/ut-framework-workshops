	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Hello World",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\examples\\01.HelloWorld\\index.js",
			"console": "integratedTerminal",
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Unit Test Index",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\examples\\02.UnitTest\\index.js",
			"console": "integratedTerminal",
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Unit Test",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\examples\\02.UnitTest\\test.js",
			"console": "integratedTerminal",
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch API",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\examples\\03.API\\index.js",
			"console": "integratedTerminal",
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch API Gateway",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\examples\\04.API Gateway\\index.js",
			"console": "integratedTerminal",
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch User Login",
			"skipFiles": [
				"<node_internals>/**"
			],
			"env": {
				"UT_ENV": "uat"
			},
			"program": "${workspaceFolder}\\examples\\06.User Login\\index.js",
			"console": "integratedTerminal",
		}
	]
}